<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
<link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
</head>
	<h1>
        <a href="/"><img src=./img/{{ .name }}.png style="margin-bottom:-0.25em;" /></a>&emsp;<a href="/">⌂</a>
	</h1>
    {{ if .vulnerable }}
    <h2>Is Vulnerable to (x2)</h2>
    {{ range $vuln := .vulnerable }} <a href="/{{ $vuln }}"><img src=./img/{{ $vuln }}.png style="{{ if or (sliceContains $.notEffective $vuln) (sliceContains $.ineffective $vuln) }}border: 4px solid #6ee073; border-radius: 30px;{{ else }}margin: 4px;{{end}}" /></a> {{ end }}
    {{end}}
    {{ if .notEffective }}
    <h2>Is NOT Effective against (x0.5)</h2>
    {{ range .notEffective }} <a href="/{{ . }}"><img src=./img/{{ . }}.png style="margin:1px" /></a> {{ end }}
    {{end}}
    {{ if .veryEffective }}
    <h2>Is Very Effective against (x2)</h2>
    {{ range .veryEffective }} <a href="/{{ . }}"><img src=./img/{{ . }}.png style="margin:1px" /></a> {{ end }}
    {{end}}
    {{ if .ineffective }}
    <h2>Completely ineffective against (x0)</h2>
    {{ range .ineffective }} <a href="/{{ . }}"><img src=./img/{{ . }}.png style="margin:1px" /></a> {{ end }}
    {{end}}
    {{ if .resistant }}
    <h2>Is Resistant to (x0.5)</h2>
    {{ range .resistant }} <a href="/{{ . }}"><img src=./img/{{ . }}.png style="margin:1px" /></a> {{ end }}
    {{end}}
    {{ if .invulnerable }}
    <h2>Completely invulnerable to (x0)</h2>
    {{ range .invulnerable }} <a href="/{{ . }}"><img src=./img/{{ . }}.png style="margin:1px" /></a> {{ end }}
    {{end}}
    <br />
    <h1><a style="right: 10; position: fixed; bottom: 10;" href="/">⌂</a></h1>

    <br />
    <small>Images from Poképedia under <a href="https://www.pokepedia.fr/Pok%C3%A9p%C3%A9dia:Copyrights">CC BY-NC-SA 3.0</a></small>
</html>
